{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "outDir": "dist",
        "strict": true,
        // "strictNullChecks": false, -> 엄격한 null검사:: 임시로 null을 넣기 위한 용도
        "moduleDetection": "force",
        "skipLibCheck": true,
    },
    "include": ["src"]
}